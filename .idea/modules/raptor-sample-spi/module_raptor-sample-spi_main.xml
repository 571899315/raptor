<?xml version="1.0" encoding="UTF-8"?>
<project name="module_raptor-sample-spi_main" default="compile.module.raptor-sample-spi_main">
  <dirname property="module.raptor-sample-spi_main.basedir" file="${ant.file.module_raptor-sample-spi_main}"/>
  
  <property name="module.jdk.home.raptor-sample-spi_main" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.raptor-sample-spi_main" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.raptor-sample-spi_main" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.raptor-sample-spi_main" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="raptor-sample-spi_main.output.dir" value="${module.raptor-sample-spi_main.basedir}/../../../raptor-sample-spi/out/production/classes"/>
  <property name="raptor-sample-spi_main.testoutput.dir" value="${module.raptor-sample-spi_main.basedir}/../../../raptor-sample-spi/out/production/classes"/>
  
  <path id="raptor-sample-spi_main.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="raptor-sample-spi_main.module.production.classpath">
    <path refid="${module.jdk.classpath.raptor-sample-spi_main}"/>
    <pathelement location="${raptor-common_main.output.dir}"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-sample-spi_main.runtime.production.module.classpath">
    <pathelement location="${raptor-sample-spi_main.output.dir}"/>
    <path refid="raptor-common_main.runtime.production.module.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-sample-spi_main.module.classpath">
    <path refid="${module.jdk.classpath.raptor-sample-spi_main}"/>
    <pathelement location="${raptor-sample-spi_main.output.dir}"/>
    <pathelement location="${raptor-common_main.output.dir}"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-sample-spi_main.runtime.module.classpath">
    <pathelement location="${raptor-sample-spi_main.output.dir}"/>
    <path refid="raptor-common_main.runtime.module.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.raptor-sample-spi_main">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.raptor-sample-spi_main">
    <patternset refid="excluded.from.module.raptor-sample-spi_main"/>
  </patternset>
  
  <path id="raptor-sample-spi_main.module.sourcepath">
    <dirset dir="${module.raptor-sample-spi_main.basedir}/../../../raptor-sample-spi/src/main">
      <include name="java"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.raptor-sample-spi_main" depends="compile.module.raptor-sample-spi_main.production,compile.module.raptor-sample-spi_main.tests" description="Compile module raptor-sample-spi_main"/>
  
  <target name="compile.module.raptor-sample-spi_main.production" depends="register.custom.compilers,compile.module.raptor-common_main" description="Compile module raptor-sample-spi_main; production classes">
    <mkdir dir="${raptor-sample-spi_main.output.dir}"/>
    <javac2 destdir="${raptor-sample-spi_main.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.raptor-sample-spi_main}/javac">
      <compilerarg line="${compiler.args.raptor-sample-spi_main}"/>
      <bootclasspath refid="raptor-sample-spi_main.module.bootclasspath"/>
      <classpath refid="raptor-sample-spi_main.module.production.classpath"/>
      <src refid="raptor-sample-spi_main.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.raptor-sample-spi_main"/>
    </javac2>
    
    <copy todir="${raptor-sample-spi_main.output.dir}">
      <fileset dir="${module.raptor-sample-spi_main.basedir}/../../../raptor-sample-spi/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.raptor-sample-spi_main.tests" depends="register.custom.compilers,compile.module.raptor-sample-spi_main.production" description="compile module raptor-sample-spi_main; test classes" unless="skip.tests"/>
  
  <target name="clean.module.raptor-sample-spi_main" description="cleanup module">
    <delete dir="${raptor-sample-spi_main.output.dir}"/>
    <delete dir="${raptor-sample-spi_main.testoutput.dir}"/>
  </target>
</project>