<?xml version="1.0" encoding="UTF-8"?>
<project name="module_raptor-common_test" default="compile.module.raptor-common_test">
  <dirname property="module.raptor-common_test.basedir" file="${ant.file.module_raptor-common_test}"/>
  
  <property name="module.jdk.home.raptor-common_test" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.raptor-common_test" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.raptor-common_test" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.raptor-common_test" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="raptor-common_test.output.dir" value="undefined"/>
  <property name="raptor-common_test.testoutput.dir" value="${module.raptor-common_test.basedir}/../../../raptor-common/out/test/classes"/>
  
  <path id="raptor-common_test.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="raptor-common_test.module.production.classpath">
    <path refid="${module.jdk.classpath.raptor-common_test}"/>
    <pathelement location="${raptor-common_main.output.dir}"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-common_test.runtime.production.module.classpath">
    <path refid="raptor-common_main.runtime.production.module.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-common_test.module.classpath">
    <path refid="${module.jdk.classpath.raptor-common_test}"/>
    <pathelement location="${raptor-common_main.output.dir}"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-common_test.runtime.module.classpath">
    <pathelement location="${raptor-common_test.testoutput.dir}"/>
    <path refid="raptor-common_main.runtime.module.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
