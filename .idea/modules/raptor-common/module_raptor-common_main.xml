<?xml version="1.0" encoding="UTF-8"?>
<project name="module_raptor-common_main" default="compile.module.raptor-common_main">
  <dirname property="module.raptor-common_main.basedir" file="${ant.file.module_raptor-common_main}"/>
  
  <property name="module.jdk.home.raptor-common_main" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.raptor-common_main" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.raptor-common_main" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.raptor-common_main" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="raptor-common_main.output.dir" value="${module.raptor-common_main.basedir}/../../../raptor-common/out/production/classes"/>
  <property name="raptor-common_main.testoutput.dir" value="${module.raptor-common_main.basedir}/../../../raptor-common/out/production/classes"/>
  
  <path id="raptor-common_main.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="raptor-common_main.module.production.classpath">
    <path refid="${module.jdk.classpath.raptor-common_main}"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-common_main.runtime.production.module.classpath">
    <pathelement location="${raptor-common_main.output.dir}"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-common_main.module.classpath">
    <path refid="${module.jdk.classpath.raptor-common_main}"/>
    <pathelement location="${raptor-common_main.output.dir}"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  <path id="raptor-common_main.runtime.module.classpath">
    <pathelement location="${raptor-common_main.output.dir}"/>
    <path refid="library.gradle:_org.springframework:spring-context:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.projectlombok:lombok:1.16.16.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-aop:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-beans:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-expression:4.1.5.release.classpath"/>
    <path refid="library.gradle:_org.springframework:spring-core:4.1.5.release.classpath"/>
    <path refid="library.gradle:_aopalliance:aopalliance:1.0.classpath"/>
    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.raptor-common_main">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.raptor-common_main">
    <patternset refid="excluded.from.module.raptor-common_main"/>
  </patternset>
  
  <path id="raptor-common_main.module.sourcepath">
    <dirset dir="${module.raptor-common_main.basedir}/../../../raptor-common/src/main">
      <include name="java"/>
      <include name="resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.raptor-common_main" depends="compile.module.raptor-common_main.production,compile.module.raptor-common_main.tests" description="Compile module raptor-common_main"/>
  
  <target name="compile.module.raptor-common_main.production" depends="register.custom.compilers" description="Compile module raptor-common_main; production classes">
    <mkdir dir="${raptor-common_main.output.dir}"/>
    <javac2 destdir="${raptor-common_main.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.raptor-common_main}/javac">
      <compilerarg line="${compiler.args.raptor-common_main}"/>
      <bootclasspath refid="raptor-common_main.module.bootclasspath"/>
      <classpath refid="raptor-common_main.module.production.classpath"/>
      <src refid="raptor-common_main.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.raptor-common_main"/>
    </javac2>
    
    <copy todir="${raptor-common_main.output.dir}">
      <fileset dir="${module.raptor-common_main.basedir}/../../../raptor-common/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.raptor-common_main.basedir}/../../../raptor-common/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.raptor-common_main.tests" depends="register.custom.compilers,compile.module.raptor-common_main.production" description="compile module raptor-common_main; test classes" unless="skip.tests"/>
  
  <target name="clean.module.raptor-common_main" description="cleanup module">
    <delete dir="${raptor-common_main.output.dir}"/>
    <delete dir="${raptor-common_main.testoutput.dir}"/>
  </target>
</project>