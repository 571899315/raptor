<?xml version="1.0" encoding="UTF-8"?>
<project name="module_raptor-sample-client" default="compile.module.raptor-sample-client">
  <dirname property="module.raptor-sample-client.basedir" file="${ant.file.module_raptor-sample-client}"/>
  
  <property name="module.jdk.home.raptor-sample-client" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.raptor-sample-client" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.raptor-sample-client" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.raptor-sample-client" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="raptor-sample-client.output.dir" value="${module.raptor-sample-client.basedir}/../../../out/production/raptor-sample-client"/>
  <property name="raptor-sample-client.testoutput.dir" value="${module.raptor-sample-client.basedir}/../../../out/test/raptor-sample-client"/>
  
  <path id="raptor-sample-client.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="raptor-sample-client.module.production.classpath">
    <path refid="${module.jdk.classpath.raptor-sample-client}"/>
  </path>
  
  <path id="raptor-sample-client.runtime.production.module.classpath">
    <pathelement location="${raptor-sample-client.output.dir}"/>
  </path>
  
  <path id="raptor-sample-client.module.classpath">
    <path refid="${module.jdk.classpath.raptor-sample-client}"/>
    <pathelement location="${raptor-sample-client.output.dir}"/>
  </path>
  
  <path id="raptor-sample-client.runtime.module.classpath">
    <pathelement location="${raptor-sample-client.testoutput.dir}"/>
    <pathelement location="${raptor-sample-client.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.raptor-sample-client">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.raptor-sample-client">
    <patternset refid="excluded.from.module.raptor-sample-client"/>
  </patternset>
  
  
  <target name="compile.module.raptor-sample-client" depends="compile.module.raptor-sample-client.production,compile.module.raptor-sample-client.tests" description="Compile module raptor-sample-client"/>
  
  <target name="compile.module.raptor-sample-client.production" depends="register.custom.compilers" description="Compile module raptor-sample-client; production classes"/>
  
  <target name="compile.module.raptor-sample-client.tests" depends="register.custom.compilers,compile.module.raptor-sample-client.production" description="compile module raptor-sample-client; test classes" unless="skip.tests"/>
  
  <target name="clean.module.raptor-sample-client" description="cleanup module">
    <delete dir="${raptor-sample-client.output.dir}"/>
    <delete dir="${raptor-sample-client.testoutput.dir}"/>
  </target>
</project>