<?xml version="1.0" encoding="UTF-8"?>
<project name="module_raptor-sample-spi" default="compile.module.raptor-sample-spi">
  <dirname property="module.raptor-sample-spi.basedir" file="${ant.file.module_raptor-sample-spi}"/>
  
  <property name="module.jdk.home.raptor-sample-spi" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.raptor-sample-spi" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.raptor-sample-spi" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.raptor-sample-spi" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="raptor-sample-spi.output.dir" value="${module.raptor-sample-spi.basedir}/../../../out/production/raptor-sample-spi"/>
  <property name="raptor-sample-spi.testoutput.dir" value="${module.raptor-sample-spi.basedir}/../../../out/test/raptor-sample-spi"/>
  
  <path id="raptor-sample-spi.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="raptor-sample-spi.module.production.classpath">
    <path refid="${module.jdk.classpath.raptor-sample-spi}"/>
  </path>
  
  <path id="raptor-sample-spi.runtime.production.module.classpath">
    <pathelement location="${raptor-sample-spi.output.dir}"/>
  </path>
  
  <path id="raptor-sample-spi.module.classpath">
    <path refid="${module.jdk.classpath.raptor-sample-spi}"/>
    <pathelement location="${raptor-sample-spi.output.dir}"/>
  </path>
  
  <path id="raptor-sample-spi.runtime.module.classpath">
    <pathelement location="${raptor-sample-spi.testoutput.dir}"/>
    <pathelement location="${raptor-sample-spi.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.raptor-sample-spi">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.raptor-sample-spi">
    <patternset refid="excluded.from.module.raptor-sample-spi"/>
  </patternset>
  
  
  <target name="compile.module.raptor-sample-spi" depends="compile.module.raptor-sample-spi.production,compile.module.raptor-sample-spi.tests" description="Compile module raptor-sample-spi"/>
  
  <target name="compile.module.raptor-sample-spi.production" depends="register.custom.compilers" description="Compile module raptor-sample-spi; production classes"/>
  
  <target name="compile.module.raptor-sample-spi.tests" depends="register.custom.compilers,compile.module.raptor-sample-spi.production" description="compile module raptor-sample-spi; test classes" unless="skip.tests"/>
  
  <target name="clean.module.raptor-sample-spi" description="cleanup module">
    <delete dir="${raptor-sample-spi.output.dir}"/>
    <delete dir="${raptor-sample-spi.testoutput.dir}"/>
  </target>
</project>