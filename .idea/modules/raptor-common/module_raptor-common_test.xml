    <path refid="library.gradle:_commons-logging:commons-logging:1.2.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.raptor-common_test">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.raptor-common_test">
    <patternset refid="excluded.from.module.raptor-common_test"/>
  </patternset>
  
  
  <target name="compile.module.raptor-common_test" depends="compile.module.raptor-common_test.production,compile.module.raptor-common_test.tests" description="Compile module raptor-common_test"/>
  
  <target name="compile.module.raptor-common_test.production" depends="register.custom.compilers,compile.module.raptor-common_main" description="Compile module raptor-common_test; production classes"/>
  
  <target name="compile.module.raptor-common_test.tests" depends="register.custom.compilers,compile.module.raptor-common_test.production" description="compile module raptor-common_test; test classes" unless="skip.tests"/>
  
  <target name="clean.module.raptor-common_test" description="cleanup module">
    <delete dir="${raptor-common_test.output.dir}"/>
    <delete dir="${raptor-common_test.testoutput.dir}"/>
  </target>
</project>