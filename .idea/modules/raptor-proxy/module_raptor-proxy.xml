<?xml version="1.0" encoding="UTF-8"?>
<project name="module_raptor-proxy" default="compile.module.raptor-proxy">
  <dirname property="module.raptor-proxy.basedir" file="${ant.file.module_raptor-proxy}"/>
  
  <property name="module.jdk.home.raptor-proxy" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.raptor-proxy" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.raptor-proxy" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.raptor-proxy" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="raptor-proxy.output.dir" value="${module.raptor-proxy.basedir}/../../../out/production/raptor-proxy"/>
  <property name="raptor-proxy.testoutput.dir" value="${module.raptor-proxy.basedir}/../../../out/test/raptor-proxy"/>
  
  <path id="raptor-proxy.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="raptor-proxy.module.production.classpath">
    <path refid="${module.jdk.classpath.raptor-proxy}"/>
  </path>
  
  <path id="raptor-proxy.runtime.production.module.classpath">
    <pathelement location="${raptor-proxy.output.dir}"/>
  </path>
  
  <path id="raptor-proxy.module.classpath">
    <path refid="${module.jdk.classpath.raptor-proxy}"/>
    <pathelement location="${raptor-proxy.output.dir}"/>
  </path>
  
  <path id="raptor-proxy.runtime.module.classpath">
    <pathelement location="${raptor-proxy.testoutput.dir}"/>
    <pathelement location="${raptor-proxy.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.raptor-proxy">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.raptor-proxy">
    <patternset refid="excluded.from.module.raptor-proxy"/>
  </patternset>
  
  
  <target name="compile.module.raptor-proxy" depends="compile.module.raptor-proxy.production,compile.module.raptor-proxy.tests" description="Compile module raptor-proxy"/>
  
  <target name="compile.module.raptor-proxy.production" depends="register.custom.compilers" description="Compile module raptor-proxy; production classes"/>
  
  <target name="compile.module.raptor-proxy.tests" depends="register.custom.compilers,compile.module.raptor-proxy.production" description="compile module raptor-proxy; test classes" unless="skip.tests"/>
  
  <target name="clean.module.raptor-proxy" description="cleanup module">
    <delete dir="${raptor-proxy.output.dir}"/>
    <delete dir="${raptor-proxy.testoutput.dir}"/>
  </target>
</project>